{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"moduleResolution": "bundler",
		"target": "ES2022",
		"lib": [
			"ES2022",
			"DOM",
			"DOM.Iterable"
		],
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"paths": {
			"@/*": [
				"./src/*"
			],
			"@/components/*": [
				"./src/components/*"
			],
			"@/lib/*": [
				"./src/lib/*"
			],
			"@/config/*": [
				"./src/config/*"
			]
		}
	}
}